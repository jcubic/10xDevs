{
  "name": "10x-devs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --turbopack && [ \"$NODE_ENV\" = \"production\" ] && prisma migrate deploy || true",
    "start": "next start",
    "lint": "eslint",
    "prettier": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "parser": "peggy --format es src/lib/parser/grammar.peggy -o src/lib/parser/parser.js",
    "postinstall": "prisma generate",
    "migrate": "node migration.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.28.0",
    "@codemirror/lang-markdown": "^6.4.0",
    "@codemirror/language-data": "^6.5.1",
    "@emotion/react": "^11.14.0",
    "@prisma/client": "^6.18.0",
    "@uiw/codemirror-theme-basic": "^4.25.2",
    "@uiw/react-codemirror": "^4.25.2",
    "better-auth": "^1.3.28",
    "clsx": "^2.1.1",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.1",
    "eslint": "^9",
    "eslint-config-next": "^16.0.0",
    "jsdom": "^27.0.1",
    "peggy": "^5.0.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5",
    "vitest": "^4.0.1"
  },
